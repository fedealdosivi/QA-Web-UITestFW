<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <sectionGroup name="capabilities">
      <section name="chrome" type="System.Configuration.AppSettingsSection, System.Configuration, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" />
      <section name="firefox" type="System.Configuration.AppSettingsSection, System.Configuration, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" />
      <section name="ie" type="System.Configuration.AppSettingsSection, System.Configuration, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" />
    </sectionGroup>
  </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7" />
    </startup>
 
    <capabilities>
      <chrome>
        <add key="os" value="Windows" />
        <add key="os_version" value="10" />
        <add key="resolution" value="1280x1024" />
        <add key="project" value= "QA_Web_UITests" />
        <add key="build" value= "Build 1.0" />
        <add key="name" value= "BasicTest" />
        <add key="browserstack.local" value= "false" />
      </chrome>
      <firefox>
        <add key="version" value="65.0 beta" />
        <add key="os" value="Windows" />
        <add key="os_version" value="10" />
        <add key="resolution" value="1280x1024" />
        <add key="project" value= "QA_Web_UITests" />
        <add key="build" value= "Build 1.0" />
        <add key="name" value= "BasicTest" />
        <add key="browserstack.local" value= "false" />
      </firefox>
      <ie>
        <add key="version" value="11.0" />
        <add key="os" value="Windows" />
        <add key="os_version" value="10" />
        <add key="resolution" value="1280x1024" />
        <add key="project" value= "QA_Web_UITests" />
        <add key="build" value= "Build 1.0" />
        <add key="name" value= "BasicTest" />
        <add key="browserstack.local" value= "false" />
      </ie>
    </capabilities>
  
  <appSettings>
    <!--  Specify the SERVER to use. Valid values: LOCAL ; BROWSERSTACK"  -->
    <add key="server_configuration" value="LOCAL" />
    <!--  Specify the Platform to use. Valid values: CHROME ; FIREFOX" ; IE   -->
    <add key="browser" value="IE" />
    <!--  WEBSITE: URL"   -->
    <add key="url" value="https://makingsense.com/" />
    <!--  BROWSER STACK CREDENTIALS  -->
    <add key="browserstack_user" value="" />
    <add key="browserstack_key" value="" />
    <add key="browserstack_server" value="hub-cloud.browserstack.com" />
    <add key="local_server" value="localhost:4444" />
  </appSettings>
</configuration>